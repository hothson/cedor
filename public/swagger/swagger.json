    "paths": {
        "/api/members": {
            "post": {
                "description": "Return Json",
                "parameters": [
                    {
                        "name": "account_number",
                        "in": "body",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "age",
                        "in": "query",
                        "required": false,
                        "type": "integer"
                    },
                    {
                        "name": "gender",
                        "in": "query",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "phone_number",
                        "in": "query",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "notes",
                        "in": "query",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "OK",
                        "examples": {
                            "application/json": {
                                "account_number": "A001",
                                "name": "Son",
                                "age": "29",
                                "gender": "Male",
                                "phone_number": "0921173391",
                                "notes": "Check me",
                                "updated_at": "2020-08-25T09:36:12.000000Z",
                                "created_at": "2020-08-25T09:36:12.000000Z",
                                "id": 2
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/auth/signup": {
            "post": {
                "description": "Return Successful Message",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "description": "name",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Your email",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "Your password",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password_confirmation",
                        "in": "query",
                        "description": "Your password",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "description": "Return access_token",
                "parameters": [
                    {
                        "name": "Content-Type",
                        "in": "header",
                        "description": "application/json",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "X-Requested-With",
                        "in": "header",
                        "description": "X-XMLHttpRequest",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "remember_me",
                        "in": "query",
                        "required": false,
                        "type": "boolean"
                    },
                    {
                        "name": "Auto Login",
                        "in": "query",
                        "required": false,
                        "type": "boolean"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "OK",
                        "examples": {
                            "application/json": {
                                "access_token": "string",
                                "token_type": "Bearer",
                                "expires_at": "string"
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/auth/logout": {
            "get": {
                "description": "Return Message",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
